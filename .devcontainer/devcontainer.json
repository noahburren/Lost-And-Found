{
  "name": "Quarkus Dev",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "runServices": [
    "app",
    "postgres"
  ],
  "shutdownAction": "stopCompose",
  "forwardPorts": [
    8080,
    5432
  ],
  "postCreateCommand": "mvn -q -DskipTests clean install",
  "remoteUser": "root"
}